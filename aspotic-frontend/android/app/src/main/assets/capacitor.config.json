{
	"appId": "io.ionic.starter",
	"appName": "Aspotic",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 3000,
			"launchAutoHide": true,
			"launchFadeOutDuration": 3000,
			"backgroundColor": "#ffffffff",
			"androidSplashResourceName": "splash",
			"showSpinner": false,
			"splashFullScreen": true,
			"splashImmersive": true
		}
	}
}
